<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.os.springcloud.dao.DeptDao">

    <select id="selectById" resultType="com.os.springcloud.entities.Dept" parameterType="java.lang.Long">
        select * from dept where deptno=#{deptno}
    </select>
</mapper>